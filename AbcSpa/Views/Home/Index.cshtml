    <!-- Using ng-cloak to avoid seeing the unstyled content -->
    <style>
        [ng-cloak] {
            display: none;
        }

        :-webkit-full-screen {
            /* properties */
            width: 100%;
            height: 100%;
            position: absolute;
            top: 0;
            left: 0;
        }

        :-moz-full-screen {
            /* properties */
            width: 100%;
            height: 100%;
            position: absolute;
        }

        :-ms-fullscreen {
            /* properties */
            position: absolute;
            width: 100%;
            height: 100%;
        }
    </style>

