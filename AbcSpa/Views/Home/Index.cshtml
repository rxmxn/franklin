<body style="padding-top: 2.5rem;" update-session-directive>

    <div id="loadingSpinners" style="display:none;">
        <div id="overlay-spinners">
            <div id="preview-area">
                <ul id="spinners">
                    <li class="three-bounce selected">
                        <div class="one"></div>
                        <div class="two"></div>
                        <div class="three"></div>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <!--<div id="workregion" class="page-content">-->
    <div id="workregion" class="flex-grid page-content">
        <div class="row" nav-directive>
            <div ui-view="header"></div>
        </div>

        <div class="row page-content" style="margin: 0;" screen-directive>
            @*<div class="col-xs-1 col-sm-1 col-md-1 col-lg-{{side_size}} sidemini"
                     style="height: 100%; padding-left: 0; padding-right: 0;"
                     ui-view="sidebar" ng-if="sidebarOn"></div>
                <div class="col-xs-11 col-sm-11 col-md-11 col-lg-{{main_size}} sidemini
                     bg-white main-scroll-limit page-content" id="cell-content" style="padding-left: 0; padding-right: 0;"
                     ui-view="content"></div>*@

            <div style="height: 100%; padding-left: 0; padding-right: 0; position: fixed; z-index: 10;"
                 ng-style="{'width': (compact === 'compact')?'2.8rem':'11rem'}"
                 ui-view="sidebar" ng-if="sidebarOn"></div>
            <div class="bg-white main-scroll-limit page-content" id="cell-content"
                 style="padding-right: 0; width: 100%;"
                 ng-style="{'padding-left': (sidebarOn === false)?0:(compact === 'compact')?'2.8rem':'11rem'}"
                 ui-view="content"></div>
        </div>
    </div>

