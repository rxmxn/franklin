<div class="col-lg-6" ng-show="panels[0].state" id="panel1" style="height: 50%;">
	<div class="panel" style="width: 100%; height: 100%;">
		<div class="heading">
			<span class="titleBarText">{{panels[0].name}}</span>
			<span class="minimize" ng-click="minimize(panels[0])"></span>
			<span class="{{allElemntsState?'home-restore':'home-full'}}" ng-click="full(panels[0])"></span>
		</div>
		<div class="content" style="height: 88%; overflow: auto;" id="panel1Content">

			<div class="col-lg-6" id="chart1">
				<div fusioncharts width="100%"
				     height="250px"
				     type="pie3d"
				     datasource="{{datasource1}}"
				     events="events1">
				</div>
			</div>

			<div class="col-lg-6" id="chart1a" data-ng-show="info1.length">
				<div fusioncharts width="100%"
					 height="250px"
					 type="pie3d"
					 datasource="{{datasource1a}}"
					 events="events1a">
				</div>
			</div>

			<label class="text-center" style="width: 100%; margin-top: 30px;">{{selectedName1}}</label>
			<div class="box-body" data-ng-if="info1.length">
				<div class="table-responsive">
					<table class="table table-hover table-condensed table-responsive">
						<thead>
						<tr role="row">
							<th>Clave Específica (Parámetro)</th>
							<th>Analistas</th>
							<th>Reconocimientos</th>
							<th>Sucursal/Instalación donde se realiza</th>
							<th>Sucursal/Instalación donde se vende</th>
						</tr>
						</thead>
						<tbody aria-relevant="all" aria-live="polite" role="alert">
						<tr class="odd" ng-repeat="ai in info1">
							<td>{{ai.Param}}</td>
							<td><span ng-repeat="a in ai.Annalists">{{a}}, </span></td>
							<td><span ng-repeat="a in ai.RecOtorgs">{{a}}, </span></td>
							<td>{{ai.SucursalRealiza}}</td>
							<td>{{ai.SucursalVende}}</td>
						</tr>
						</tbody>
					</table>
				</div>
			</div>

			<label class="text-center" style="width: 100%; margin-top: 30px;" data-ng-if="info1a.length">
				{{selectedName1a}}, Reconocimiento: {{info1a[0].Ack}}
			</label>
			<div class="box-body" data-ng-if="info1a.length">
				<div class="table-responsive">
					<table class="table table-hover table-condensed table-responsive">
						<thead>
							<tr role="row">
								<th>Clave Específica (Parámetro)</th>
								<th>Analistas</th>
								<th>Sucursal/Instalación donde se realiza</th>
								<th>Sucursal/Instalación donde se vende</th>
							</tr>
						</thead>
						<tbody aria-relevant="all" aria-live="polite" role="alert" ng-repeat="i in info1a">
							<tr class="odd" ng-repeat="j in i.Param">
								<td>{{j.Name}}</td>
								<td><span ng-repeat="a in j.Analistas">{{a}}, </span></td>
								<td>{{j.SucursalRealiza}}</td>
								<td>{{j.SucursalVende}}</td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>

		</div>
	</div>
</div>

