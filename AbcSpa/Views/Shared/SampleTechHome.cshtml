				<div class="table-responsive">
					<table class="table table-hover table-condensed table-responsive">
						<thead>
						<tr role="row">
							<th style="width: 5%; text-align: center;">Foto</th>
							<th>Nombre</th>
							<th>Reconocimientos</th>
							<th>Sucursales</th>
						</tr>
						</thead>
						<tbody aria-relevant="all" aria-live="polite" role="alert">
						<tr class="odd" ng-repeat="sig in info3">
							<td style="text-align: center !important;">
								<img data-ng-if="sig.Photo === '' || sig.Photo === null" ng-src="/Content/img/{{sig.Gender === false ? 'usermale.jpg' : 'female.jpg' }}" style="width: 42px; height: 42px;" />
								<img data-ng-if="sig.Photo !== '' && sig.Photo !== null" ng-src="{{sig.Photo}}" style="width: auto; height: 42px;" />
							</td>
							<td>{{sig.Name}}</td>
							<td><span ng-repeat="ack in sig.acks">{{ack}}, </span></td>
							<td>
								<ul style="margin-left: 0; padding-left: 1rem;">
									<li ng-repeat="suc in sig.sucursales">{{suc}}</li>
								</ul>
							</td>
						</tr>
						</tbody>
					</table>
				</div>
			</div>

			<label class="text-center" style="width: 100%; margin-top: 30px;">{{selectedName3a}}</label>
