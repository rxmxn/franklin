<div class="main-scroll">
    <div class="padding10" style="position: absolute; width: 99%;">
        <h6 class="text-light margin0 text-uppercase">
            Elementos Gr&aacute;ficos
            <!-- <span class="ani-hover-spin ani-slow icon mif-help place-right"></span>  -->
        </h6>
        <hr class="thin bg-grayLighter">
    </div>

	<div class="col-lg-12" minimize-home-directive id="rightPanel" style="height: 100%; padding-top: 2rem;">
		<div ng-if="address === 'admin'">@Html.Partial("AdminHome")</div>
		<div ng-if="address === 'sampletech'" style="height: 100%; padding-bottom: 2.5rem;">@Html.Partial("SampleTechHome")</div>
