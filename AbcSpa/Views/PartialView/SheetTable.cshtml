                                                                <label class="input-control checkbox block">
                                                                    <input type="checkbox" ng-model="selection.selected"
                                                                           data-ng-click="selectElement(selection, 'clasquim1')"
                                                                           data-placement="right">
                                                                    <span class="caption col-md-10" style="margin: 0; padding: 0;">
                                                                        <span class="col-md-12">
                                                                            {{selection.name}}
                                                                        </span>
                                                                    </span>
                                                                    <span class="check pull-right col-md-1"></span>
                                                                </label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="col-lg-4">
                                                    <div class="dropdown-button" style="height: 1.61rem; margin-top: 0.455rem; width: 100%; float: left;">
                                                        <button class="button dropdown-toggle" style="height: inherit; width: inherit;"
                                                                ng-disabled="!(clasquims2.length>0)">
                                                            Nivel 2
                                                        </button>
                                                        <div class="split-content d-menu padding10 place-right" data-role="dropdown" data-no-close="true" style="width: 350px; max-width: 500px;">
                                                            <div class="clearfix" data-ng-repeat="selection in clasquims2">
                                                                <label class="input-control checkbox block">
                                                                    <input type="checkbox" ng-model="selection.selected"
                                                                           data-ng-click="selectElement(selection, 'clasquim2')"
