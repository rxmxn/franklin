                                                    <input type="text" placeholder="Búsqueda por Descripción específica."
                                                           data-ng-model="specificDescription"
                                                           list="sugSpecDesc"
                                                           data-ng-change="getSpecificDescriptionSug()"
                                                           style="line-height: inherit;">
                                                    <datalist id="sugSpecDesc">
                                                        <option data-ng-repeat="specificDesc in specificDescList" value="{{specificDesc}}"></option>
                                                    </datalist>
                                                </div>

                                            </div>
                                            <div class="col-lg-4">

                                                <div class="input-control text" style=" width: 100%; margin: 0;">
                                                   <input type="text" list="sugMethod" placeholder="Búsqueda por método"
                                                           style="border-radius: 0; line-height: inherit;"
                                                           data-ng-model="searchMethod"
                                                           data-ng-change="" />
                                                   <datalist id="sugMethod">
                                                        <option data-ng-repeat="met in methodList" value="{{met}}"></option>
                                                    </datalist>
                                                </div>

                                                <div class="input-control text" style="width: 100%; margin: 5px 0 0 0;">
                                                   <input type="text" list="sugGenericKey" placeholder="Búsqueda por clave genérica"
                                                           style="border-radius: 0; line-height: inherit;"
                                                           data-ng-model="searchGenericKey"
                                                           data-ng-change="" />
                                                    <datalist id="sugGenericKey">
                                                        <option data-ng-repeat="genericKey in genericKeyList" value="{{genericKey}}"></option>
                                                    </datalist>
                                                </div>

                                                <div class="input-control text" style="float: right; ">
                                                    <button class="button" data-ng-click="ResetFilter()"
                                                            style="float: right; position: relative;"
                                                            data-toggle="tooltip" title="Borrar campos de búsqueda">
                                                        <span class="mif-cross"></span>
                                                    </button>

                                                    <button class="button" data-ng-click="RefreshList(1)"
                                                            style="float: right; margin-right: 5px; position: relative;"
                                                            data-toggle="tooltip" title="Buscar en Base de datos">
                                                        <span class="mif-search"></span>
                                                    </button>
                                                </div>
                                                
                                            </div>

                                        </div>
                                        <!-- Termina aqui el desplegable de filtros -->
                                    </div>
                                </div>
                            </div>

                            <span class="titleBarText" style="margin-left: 1.825rem;">{{panels[1].name}}</span>
                            <span class="minimize" ng-if="!isFullscreen" ng-click="minimize(panels[1])"
                                  style="right: 2.625rem;"
                                  data-toggle="tooltip" title="Minimizar Tabla Principal"></span>
                            <span class="{{isFullscreen?'restoreDown':'maximize'}}" ng-click="toggleFullScreen()"
                                  data-toggle="tooltip" title="{{isFullscreen?'Restaurar Tabla Principal':'Maximizar Tabla Principal'}}"></span>
                        </div>
                        @* ng-style="{'height': !panels[0].state?'94%':'90%'}" *@
                        <div class="content" id="panelBody" style="padding: 0; overflow: auto; height: 94%;">
                            <div class="box-body" data-ng-if="!elementsList.length" style="padding: 5px;">
                                <div class="scroll-panel margin-left-10">
                                    <p data-ng-if="isLoading">
                                        <i class="fa fa-spinner fa-spin" style="color: black;"></i> Cargando Datos. Espere, por favor.
                                    </p>
                                    <p data-ng-if="!isLoading">
                                        <i class="glyphicon glyphicon-exclamation-sign" style="color: dimgray;"></i> No existen resultados.
                                    </p>
                                </div>
                            </div>
                            <!-- Elements Table -->
                            <div id="panelBody2" class="box-body" data-ng-if="elementsList.length"
                                 ng-style="{'height': (pageTotal<=10)? '96%': '90%'}">

                                <div class="row" style="margin: 0;">
                                    @*<div class="box-header" style="padding: 0; ">*@
                                    <div class="box-name tableActiveDeactiveElementText">
                                        <span class="icon mif-flow-cascade"></span>
                                        <span style="color: rgb(31, 31, 32); text-transform: uppercase; text-shadow: 0px 1px 0px rgb(255, 255, 255); font-size: 0.75rem; margin-left: 5px;">
                                            Elementos: {{pageTotal}}
                                        </span>
                                    </div>
                                    @*</div>*@

                                    <div class="box-tools pull-right" style="margin-right: 0.2rem;">
                                        @*<button ng-disabled="reporting" ng-click="" class="button primary">Crear reporte</button>*@
                                        <div ng-if="!isFullscreen" class="box-name tableButtons">
                                            <a href="" data-ng-click="printReportDialog(true)" data-toggle="tooltip" title="{{(generatingEXCELFile || generatingPDFFile)?'Generando archivo, por favor espere':'Generar archivo excel'}}">
                                                <span class="icon {{(generatingEXCELFile)?'mif-spinner mif-ani-spin':'mif-file-excel'}} iconStyle"></span>
                                            </a>
                                        </div>
                                        <div ng-if="!isFullscreen" class="box-name tableButtons" data-ng-disabled="generatingPDFFile || generatingEXCELFile">
                                            <a href="" data-ng-click="printReportDialog(false)" data-toggle="tooltip" title="{{(generatingPDFFile || generatingEXCELFile)?'Generando archivo, por favor espere':'Generar archivo pdf'}}">
                                                <span class="icon  {{(generatingPDFFile)?'mif-spinner mif-ani-spin':'mif-file-pdf'}} iconStyle"></span>
                                            </a>
                                        </div>
                                        <div class="box-name tableButtons">
                                            <a href="" data-ng-click="my_tree.expand_all()" data-toggle="tooltip" title="Expandir todo">
                                                <span class="icon  mif-enlarge2 iconStyle"></span>
                                            </a>
                                        </div>
                                        <div class="box-name tableButtons">
                                            <a href="" data-ng-click="my_tree.collapse_all()" data-toggle="tooltip" title="Contraer todo">
                                                <span class="icon  mif-shrink2 iconStyle"></span>
                                            </a>
                                        </div>
                                    </div>

                                </div>
                                <hr class="thin bg-grayLighter" style="margin-top: 0.3rem;">

                                <div id="tableContainer" class="box-body" style="padding: 0; overflow: auto;"
                                     ng-style="{'height': (pageTotal<=10)? '100%': '95%'}">
                                    <div ng-style="{'width': tableWidth+'px'}">
                                        <tree-grid tree-data="elementsList"
                                                   tree-control="my_tree"
                                                   col-defs="col_defs"
                                                   expand-on="expanding_property"
                                                   on-select="my_tree_handler(branch)"
                                                   expand-level="3"
                                                   icon-expand="icon mif-chevron-thin-right mif-s"
                                                   icon-collapse="icon mif-chevron-thin-down mif-s"
                                                   icon-leaf="icon mif-lab">
                                        </tree-grid>
                                    </div>
                                </div>
                            </div>

                            <!-- End of Elements Table -->

                            <section ng-show="pageTotal > 10">
                                @Html.Partial("BoxFooter")
                            </section>

                        </div>
                    </div>

                </div>

            </div>
        </div>
    </div>
    <div class="clearfix"></div>
    <ul class="t-menu horizontal compact" style="width: 100%; position:absolute; bottom: 0; z-index: 999;" ng-if="taskbar">
        <li ng-repeat="p in panels" style="margin-right: 3px;">
            <a href="" ng-if="!p.state" ng-click="minimize(p)">{{p.name}}</a>
        </li>
    </ul>

</div>
